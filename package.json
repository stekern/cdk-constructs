{
  "name": "@stekern/cdk-constructs",
  "repository": {
    "type": "git",
    "url": "https://github.com/stekern/cdk-constructs.git"
  },
  "version": "0.0.0-development",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc --noEmit && tsc --project tsconfig.build.json && npm run copy-assets",
    "copy-assets": "find assets -type f -not -name '*.snap' -not -name '*.spec.ts' -not -name 'mocks.ts' -not -name 'types.ts' -exec bash -c 'path=\"{}\" && mkdir -p \"dist/${path%/*}\" && cp \"$path\" \"dist/$path\"' ';'",
    "test": "jest"
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "dev",
        "channel": "alpha",
        "prerelease": true
      }
    ]
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "3.588.0",
    "@aws-sdk/client-kms": "3.588.0",
    "@aws-sdk/client-secrets-manager": "3.588.0",
    "@aws-sdk/lib-dynamodb": "3.588.0",
    "@aws-sdk/util-dynamodb": "3.588.0",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@octokit/types": "7.5.1",
    "@octokit/webhooks-types": "6.11.0",
    "@tsconfig/node16": "16.1.3",
    "@types/aws-lambda": "8.10.141",
    "@types/jest": "29.5.12",
    "@types/node": "16.18.97",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "aws-cdk": "2.144.0",
    "aws-cdk-lib": "2.144.0",
    "aws-sdk": "2.1632.0",
    "constructs": "10.3.0",
    "esbuild": "0.21.4",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "3.3.3",
    "semantic-release": "24.0.0",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}
